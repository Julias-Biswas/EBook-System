<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>SignUpServlet</servlet-name>
        <servlet-class>com.user.servlet.SignUpServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignInServlet</servlet-name>
        <servlet-class>com.user.servlet.SignInServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddBooks</servlet-name>
        <servlet-class>com.admin.servlet.AddBooks</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditBooks</servlet-name>
        <servlet-class>com.admin.servlet.EditBooks</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteBooks</servlet-name>
        <servlet-class>com.admin.servlet.DeleteBooks</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>com.user.servlet.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CardServlet</servlet-name>
        <servlet-class>com.user.servlet.CardServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RemoveBookCartServlet</servlet-name>
        <servlet-class>com.user.servlet.RemoveBookCartServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SellOldBookServlet</servlet-name>
        <servlet-class>com.user.servlet.SellOldBookServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteOldBookServlet</servlet-name>
        <servlet-class>com.user.servlet.DeleteOldBookServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditOldBookServlet</servlet-name>
        <servlet-class>com.user.servlet.EditOldBookServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditUserProfileServlet</servlet-name>
        <servlet-class>com.user.servlet.EditUserProfileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddOrUpdateUserAdderssServlet</servlet-name>
        <servlet-class>com.user.servlet.AddOrUpdateUserAdderssServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OrderServlet</servlet-name>
        <servlet-class>com.user.servlet.OrderServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SignUpServlet</servlet-name>
        <url-pattern>/SignUpServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignInServlet</servlet-name>
        <url-pattern>/SignInServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddBooks</servlet-name>
        <url-pattern>/AddBooks</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditBooks</servlet-name>
        <url-pattern>/EditBooks</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteBooks</servlet-name>
        <url-pattern>/DeleteBooks</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CardServlet</servlet-name>
        <url-pattern>/CardServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RemoveBookCartServlet</servlet-name>
        <url-pattern>/RemoveBookCartServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SellOldBookServlet</servlet-name>
        <url-pattern>/SellOldBookServlet</url-pattern>
    </servlet-mapping>
